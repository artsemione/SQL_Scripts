CREATE TABLE LOGDosagem(
	DBTimeStamp datetime2 PRIMARY KEY,
	Usuario VARCHAR(20),
  	Id INT,
 	Event VARCHAR(20),
  	Hora datetime2(0),		
	Vazao decimal(3, 0),
  	Sulfato decimal(3, 0), 
  	[Sulfato%] decimal(3, 0), 
  	GCP decimal(3, 0),
  	[GCP%] decimal(3, 1),
  	GCS decimal(3, 0),
	[GCS%] decimal(3, 1),
  	Fluor decimal(3, 0),
  	[Fluor%] decimal(3, 1),
  	Cloro decimal(3, 0),
  	Operador VARCHAR(20)
  );
